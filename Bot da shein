from aiogram import Bot, Dispatcher
from aiogram.utils import executor
import asyncio

TOKEN = "SEU_TOKEN"
bot = Bot(TOKEN)
dp = Dispatcher(bot)

CHANNELS = ["@canal1", "@canal2"]
MSG = """ğŸ”¥ SHEIN COM DESCONTO
ğŸŸ Cupom: QEQ8X
ğŸ‘‰ https://onelink.shein.com/28/5fkyzat3xxrv"""

async def repost():
    while True:
        for ch in CHANNELS:
            await bot.send_message(ch, MSG)
        await asyncio.sleep(21600)  # 5min

if __name__ == "__main__":
    loop = asyncio.get_event_loop()
    loop.create_task(repost())
    executor.start_polling(dp)

# bot.py
from aiogram import Bot, Dispatcher
from aiogram.utils import executor
import asyncio, sqlite3, time

TOKEN = "SEU_TOKEN"
BASE_LINK = "https://onelink.shein.com/28/5fkyzat3xxrv"
CUPOM = "QEQ8X"
CHANNELS = ["@canal1", "@canal2"]

bot = Bot(TOKEN)
dp = Dispatcher(bot)

db = sqlite3.connect("clicks.db")
db.execute("""
CREATE TABLE IF NOT EXISTS logs (
id INTEGER PRIMARY KEY,
channel TEXT,
utm TEXT,
timestamp INTEGER
)
""")
db.commit()

def gen_link(channel):
    utm = f"{channel}_{int(time.time())}"
    link = f"{BASE_LINK}?utm_source=telegram&utm_campaign={utm}"
    db.execute("INSERT INTO logs(channel, utm, timestamp) VALUES (?,?,?)",
               (channel, utm, int(time.time())))
    db.commit()
    return link

async def repost():
    while True:
        for ch in CHANNELS:
            link = gen_link(ch.replace("@",""))
            msg = f"""ğŸ”¥ SHEIN COM DESCONTO
ğŸŸ Cupom: {QEQ8X}
â³ AGORA
ğŸ‘‰ {link}""https://onelink.shein.com/28/5fkyzat3xxrv"
            await bot.send_message(ch, msg)
        await asyncio.sleep(21600)  # 5min

if __name__ == "__main__":
    loop = asyncio.get_event_loop()
    loop.create_task(repost())
    executor.start_polling(dp)

# panel.py
from flask import Flask
import sqlite3
/projeto
 â”œ bot.py
 â”œ shortener.py
 â”œ panel.py
 â”œ clicks.db
app = Flask(__name__)

@app.route("/")
def dashboard():
    db = sqlite3.connect("clicks.db")
    cur = db.cursor()
    cur.execute("SELECT channel, COUNT(*) FROM logs GROUP BY channel")
    data = cur.fetchall()
    html = "<h1>Cliques por Canal</h1><ul>"
    for c, n in data:
        html += f"<li>{c}: {n}</li>"
    html += "</ul>"
    return html

app.run(host="0.0.0.0", port=5000)

